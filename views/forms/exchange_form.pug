        ol
          li Sucursales de Chilexpress.
          li Sucursales de Correos Chile.
          li Sucursales de .....
.form_to_save
  .row_item_prod
    .title 
      h4 Información Bancaria
    form(method="post")
      .form-group
        label(for="account_holder_name") Nombre del titular de la cuenta:
        input#account_holder_name(type="text", name="account_holder_name")
      .form-group
        label(for="account_holder_rut") RUT del titular de la cuenta:
        input#account_holder_rut(type="text", name="account_holder_rut")
      .form-group
        label(for="account_holder_email") Email del titular de la cuenta:
        input#account_holder_email(type="email", name="account_holder_email")
      .form-group
        label(for="account_bank") Banco:
        //-input#account_bank(type="text", name="account_bank")
        select#account_bank(name="account_bank")
          option Seleccione
          option(value="1") Banco Santander
          option(value="2") Ripley
          option(value="3") BCP
      .form-group
        label(for="account_type_bank") Tipo de cuenta:
        //-input#account_type_bank(type="text", name="account_type_bank")
        select#account_type_bank(name="account_type_bank")
          option Seleccione
          option(value="1") Visa
          option(value="2") Mastercard
          option(value="3") Amex
      .form-group
        label(for="account_number_bank") Numero de cuenta:
        input#account_number_bank(type="text", name="account_number_bank")
      .form-group
        label(for="courier_exchange") Courier:
        select#courier_exchange(name="courier_exchange")
          option(value="1") Correos Chile
          option(value="2") Chile Express 
      .form-buttons
        input(type="submit" id="send_exchange_body" value="Continuar").btnPrimary